{
	"expo": {
		"name": "Fokuz",
		"slug": "Fokuz",
		"version": "1.0.0",
		"orientation": "default",
		"icon": "./assets/images/icon.png",
		"scheme": "fokuz",
		"userInterfaceStyle": "automatic",
		"newArchEnabled": true,
		"ios": {
			"supportsTablet": true,
			"infoPlist": {
				"UIBackgroundModes": ["remote-notification"]
			}
		},
		"android": {
			"edgeToEdgeEnabled": true,
			"predictiveBackGestureEnabled": false,
			"package": "com.marineg404.Fokuz",
			"permissions": ["RECEIVE_BOOT_COMPLETED", "VIBRATE", "POST_NOTIFICATIONS"]
		},
		"web": {
			"output": "static",
			"favicon": "./assets/images/favicon.png"
		},
		"plugins": [
			"expo-router",
			[
				"expo-splash-screen",
				{
					"image": "./assets/images/splash-icon.png",
					"imageWidth": 200,
					"resizeMode": "contain",
					"backgroundColor": "#F5F5F7",
					"dark": {
						"backgroundColor": "#0b0b0d"
					}
				}
			],
			[
				"expo-notifications",
				{
					"icon": "./assets/images/icon.png",
					"color": "#ffffff",
					"sounds": [],
					"mode": "production"
				}
			]
		],
		"experiments": {
			"typedRoutes": true,
			"reactCompiler": true
		},
		"extra": {
			"router": {},
			"eas": {
				"projectId": "23f2cc50-0a3c-4efa-ba3c-f76ac42d52d9"
			}
		}
	}
}
