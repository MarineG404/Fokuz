{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"strict": true,
		"paths": {
			"@/*": [
				"./*"
			]
		},
		"resolveJsonModule": true,
		"esModuleInterop": true
	},
	"include": [
		"**/*.ts",
		"**/*.tsx",
		".expo/types/**/*.ts",
		"expo-env.d.ts",
		"src/localization/i18n.ts",
		"env.d.ts"
	]
}
